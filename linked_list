class Node {
    private:
        int val;
        Node* next;
    public:
        Node(int);
        int get_value();
        Node* get_next();
        bool set_next(Node*);
        ~Node();
};

class linked_list
{
private:
    Node* head;
    bool is_empty;
public:
    linked_list();
    ~linked_list();
    bool insert_node(Node*);
    bool print_list();
};

